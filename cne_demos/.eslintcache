r\n\r\n        setTimeout( () => { setShowDetails(false)}, 5000);\r\n    });\r\n\r\n    return (\r\n        <>\r\n            <form>\r\n                <label> Brand:</label>\r\n                <input\r\n                    type=\"text\"\r\n                    value={brand}\r\n                    onChange={e => setBrand(e.target.value)} />\r\n                <label> Model: </label>\r\n                <input\r\n                    type=\"text\"\r\n                    value={model}\r\n                    onChange={e => setModel(e.target.value)} />\r\n                <label>\r\n                    colour\r\n                </label>\r\n                <select onClick={e => setColour(e.target.value)}>\r\n                    <option defaultValue={colour}>{colour}</option>\r\n                    <option value=\"blue\">Blue</option>\r\n                    <option value=\"green\">Green</option>\r\n                    <option value=\"red\">Red</option>\r\n                    <option value=\"orange\">Orange</option>\r\n                </select>\r\n                <label>Year</label>\r\n                <input\r\n                    type=\"text\"\r\n                    value={year}\r\n                    onChange={e => setYear(e.target.value)} />\r\n                <button type=\"submit\" onClick={callMe}>Submit</button>\r\n            </form>\r\n            { showDetails ? <p> {brand}, {model}, {colour}, {year} </p> : <></>}\r\n        </>\r\n    )\r\n}\r\n\r\nexport default Car; ","C:\\Users\\Savannah-Vaith\\Documents\\QAC\\Cohorts\\CNE\\20SepCNative1\\cne_demos\\src\\Components\\04-State\\Child.jsx",[],"C:\\Users\\Savannah-Vaith\\Documents\\QAC\\Cohorts\\CNE\\20SepCNative1\\cne_demos\\src\\Components\\05-forms\\Basic.jsx",[],"C:\\Users\\Savannah-Vaith\\Documents\\QAC\\Cohorts\\CNE\\20SepCNative1\\cne_demos\\src\\Components\\05-forms\\ComponentWithUncontrolledInput.jsx",[],"C:\\Users\\Savannah-Vaith\\Documents\\QAC\\Cohorts\\CNE\\20SepCNative1\\cne_demos\\src\\Components\\05-f